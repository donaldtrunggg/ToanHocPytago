<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block page_title %}Toán Học Pytago{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
          integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <!-- Font Awesome -->

    <!-- Ionicons -->
    {#<link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">#}
    <!-- Google material icon font -->
    <link href="//fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">

    <link href="//fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900,100italic,300italic,400italic,500italic,700italic,900italic&subset=latin,latin-ext,vietnamese"
          rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    {% block stylesheets %}
        {% stylesheets filter="scssphp"
        "@AppBundle/Resources/public/back_office/vendors/bootstrap/dist/css/bootstrap.min.css"
        "@AppBundle/Resources/public/back_office/vendors/font-awesome/css/font-awesome.min.css"
        "@AppBundle/Resources/public/back_office/vendors/nprogress/nprogress.css"
        "@AppBundle/Resources/public/back_office/vendors/iCheck/skins/flat/green.css"
        "@AppBundle/Resources/public/back_office/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css"
        "@AppBundle/Resources/public/back_office/vendors/jqvmap/dist/jqvmap.min.css"
        "@AppBundle/Resources/public/back_office/vendors/bootstrap-daterangepicker/daterangepicker.css"
        "@AppBundle/Resources/public/back_office/css/lib/custom.min.css"

        output="bundles/app/back_office/css/custom_admin.css" %}
        {% include '@App/components/twig_css_asset.html.twig' %}
        {% endstylesheets %}
    {% endblock %}

    <script>
        {% block javascript_global_setting %}

        {% endblock %}
    </script>

    {% block top_javascripts %}
    {% endblock %}

</head>
<body class="{% block body_class %}{% endblock %}" {% block body_attr %}{% endblock %}>

{% block body %}{% endblock %}

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

{% block javascripts %}
    {% javascripts filter="?jsqueeze"
    "@AppBundle/Resources/public/back_office/vendors/jquery/dist/jquery.min.js"
    "@AppBundle/Resources/public/back_office/vendors/bootstrap/dist/js/bootstrap.min.js"
    "@AppBundle/Resources/public/back_office/vendors/fastclick/lib/fastclick.js"
    "@AppBundle/Resources/public/back_office/vendors/nprogress/nprogress.js"
    "@AppBundle/Resources/public/back_office/vendors/Chart.js/dist/Chart.min.js"
    "@AppBundle/Resources/public/back_office/vendors/gauge.js/dist/gauge.min.js"
    "@AppBundle/Resources/public/back_office/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"
    "@AppBundle/Resources/public/back_office/vendors/iCheck/icheck.min.js"
    "@AppBundle/Resources/public/back_office/vendors/skycons/skycons.js"
    "@AppBundle/Resources/public/back_office/vendors/Flot/jquery.flot.js"
    "@AppBundle/Resources/public/back_office/vendors/Flot/jquery.flot.pie.js"
    "@AppBundle/Resources/public/back_office/vendors/Flot/jquery.flot.time.js"
    "@AppBundle/Resources/public/back_office/vendors/Flot/jquery.flot.stack.js"
    "@AppBundle/Resources/public/back_office/vendors/Flot/jquery.flot.resize.js"
    "@AppBundle/Resources/public/back_office/vendors/flot.orderbars/js/jquery.flot.orderBars.js"
    "@AppBundle/Resources/public/back_office/vendors/flot-spline/js/jquery.flot.spline.min.js"
    "@AppBundle/Resources/public/back_office/vendors/flot.curvedlines/curvedLines.js"
    "@AppBundle/Resources/public/back_office/vendors/DateJS/build/date.js"
    "@AppBundle/Resources/public/back_office/vendors/jqvmap/dist/jquery.vmap.js"
    "@AppBundle/Resources/public/back_office/vendors/jqvmap/dist/maps/jquery.vmap.world.js"
    "@AppBundle/Resources/public/back_office/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"
    "@AppBundle/Resources/public/back_office/vendors/moment/min/moment.min.js"
    "@AppBundle/Resources/public/back_office/vendors/bootstrap-daterangepicker/daterangepicker.js"
    "@AppBundle/Resources/public/back_office/js/lib/custom.min.js"

    output="bundles/app/back_office/js/custom_admin.js" %}
    {% include '@App/components/twig_js_asset.html.twig' %}
    {% endjavascripts %}
{% endblock %}

</body>
</html>
